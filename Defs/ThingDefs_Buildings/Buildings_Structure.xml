<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef Name="WhitePicketFence" ParentName="BuildingBase">
        <defName>WhitePicketFence</defName>
        <label>White Picket Fence</label>
        <description>What a pretty fence!</description>
        <category>Building</category>
        <thingClass>Building</thingClass>

        <graphicData>
            <texPath>Things/Structures/WhitePicketFence/WPFenceAtlas</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <linkType>CornerFiller</linkType>
            <linkFlags>
                <li>Wall</li>
                <li>Rock</li>
            </linkFlags>
        </graphicData>
        <uiIconPath>Things/Structures/WhitePicketFence/WPFenceIcon</uiIconPath>

        <statBases>
            <MaxHitPoints>10</MaxHitPoints>
            <WorkToBuild>20</WorkToBuild>
            <Flammability>1.0</Flammability>
            <Beauty>9</Beauty>
        </statBases>
        <costStuffCount>2</costStuffCount>
        <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
        <altitudeLayer>Building</altitudeLayer>
        <passability>PassThroughOnly</passability>
        <blockWind>true</blockWind>
        <castEdgeShadows>true</castEdgeShadows>
        <fillPercent>1</fillPercent>
        <coversFloor>false</coversFloor>
        <placingDraggableDimensions>1</placingDraggableDimensions>
        <tickerType>Never</tickerType>
        <rotatable>true</rotatable>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
        <holdsRoof>false</holdsRoof>
        <designationCategory>Structure</designationCategory>
        <staticSunShadowHeight>0.5</staticSunShadowHeight>
        <blockLight>false</blockLight>
        <canOverlapZones>true</canOverlapZones>
        <costList>
            <WoodLog>1</WoodLog>
            <Steel>1</Steel>
        </costList>
        <building>
            <isInert>true</isInert>
            <ai_chillDestination>false</ai_chillDestination>
            <blueprintGraphicData>
                <texPath>Things/Structures/WhitePicketFence/WPFenceBlueprintAtlas</texPath>
            </blueprintGraphicData>
        </building>
    </ThingDef>
</Defs>