<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef Name="Waitrose" ParentName="BuildingBase">
        <defName>Waitrose</defName>
        <label>Waitrose Sign</label>
        <description>A middle class favourite.</description>

        <thingClass>Building</thingClass>
        <category>Building</category>
        <altitudeLayer>Building</altitudeLayer>
        <passability>Impassable</passability>
        <selectable>true</selectable>
        <fillPercent>0.20</fillPercent>
        <pathCost>8</pathCost>
        <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
        <designationCategory>Misc</designationCategory>
        <rotatable>false</rotatable>
        <graphicData>
            <graphicClass>Graphic_Single</graphicClass>
            <texPath>Things/Misc/Signs/Waitrose</texPath>
        </graphicData>
        <comps>
            <li Class="CompProperties_Glower">
                <glowRadius>5</glowRadius>
                <glowColor>(109,174,0,0)</glowColor>
            </li>
        </comps>
        <statBases>
            <MaxHitPoints>20</MaxHitPoints>
            <WorkToBuild>10</WorkToBuild>
            <Flammability>1.0</Flammability>
            <Beauty>1</Beauty>
        </statBases>
        <clearBuildingArea>false</clearBuildingArea>
        <building>
            <canPlaceOverWall>true</canPlaceOverWall>
        </building>
    </ThingDef>
    
